genfscon debugfs /rmt_storage                         u:object_r:debugfs_rmt:s0
genfscon debugfs /sys/kernel/debug/rmt_storage        u:object_r:debugfs_rmt:s0
genfscon debugfs /rfs_access                          u:object_r:debugfs_rfs:s0
